{
  "name": "vastly",
  "version": "0.0.1",
  "description": "Everything you need to support a custom expression language",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "node test/index.js",
    "build:html": "npx @11ty/eleventy --config=_build/.eleventy.cjs",
    "build": "npm run build:html",
    "watch:html": "npx @11ty/eleventy --config=_build/.eleventy.cjs --watch",
    "watch": "npm run watch:html",
    "build:docs": "npx typedoc --out docs src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mavoweb/vastly.git"
  },
  "keywords": [
    "AST",
    "parsing",
    "expressions"
  ],
  "author": "Lea Verou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mavoweb/vastly/issues"
  },
  "homepage": "https://github.com/mavoweb/vastly#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "eleventy-plugin-toc": "^1.1.5",
    "glob": "^10.3.10",
    "htest.dev": "^0.0.4",
    "jsep": "^1.3.8",
    "markdown-it-anchor": "^8.6.7",
    "typedoc": "^0.25.3"
  }
}
